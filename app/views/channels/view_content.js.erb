<% if grant_access('view_channels', current_user)%>
$("#channel-detail").replaceWith("<%= escape_javascript(render(:partial => 'channels/detail', locals: {channel: @channel, page:@page })) %>");
$("li").removeClass("bg-light-grey");
$("#<%= @rnd%>").addClass( "bg-light-grey" );
$("#<%= @rnd%>-label").css('display','none');
$("#cm").replaceWith("<%= escape_javascript(render(:partial => 'comments/remote_form', locals: {c: @channel, page:@page })) %>");
<%end%>
