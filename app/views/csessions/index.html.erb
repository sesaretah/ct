<p id="notice"><%= notice %></p>

<h1>Listing Csessions</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @csessions.each do |csession| %>
      <tr>
        <td><%= csession.course_id %></td>
        <td><%= csession.title %></td>
        <td><%= csession.description %></td>
        <td><%= link_to 'Show', csession %></td>
        <td><%= link_to 'Edit', edit_csession_path(csession) %></td>
        <td><%= link_to 'Destroy', csession, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Csession', new_csession_path %>
